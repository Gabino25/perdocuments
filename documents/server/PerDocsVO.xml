   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT V.PERSON_ID,
          V.ASSIGNMENT_ID,
          V.EMPLOYEE_NUMBER,
          V.FECHA_CONTRATACION,
          V.FECHA_BAJA,
          V.FULL_NAME,
          V.NAME,
          V.POSITION_ID,
          XPD.ID,
          XPD.CREATION_DATE,
          XPD.EFECTIVE_DATE,
          XPD.STATUS
     FROM APPS.XXAZOR_PER_CARTA_LABORAL_V V
         ,XXAZOR_PER_DOCS XPD
    WHERE V.PERSON_ID = XPD.PERSON_ID
      AND V.ASSIGNMENT_ID = XPD.ASSIGNMENT_ID
   ]]></SQLQuery>
